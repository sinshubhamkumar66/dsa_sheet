class Solution {
    public void reverseString(char[] s) {
        int len = s.length;
        for(int i=0;i<s.length/2;i++)
        {
            char temp = s[i];
            s[i] = s[len-i-1];
            s[len-i-1] = temp;
        }
        
    }
}
// second solution in c++

class Solution {
public:
    void reverseString(vector<char>& s) {
        reverse(s.begin(), s.end());
        
    }
};
